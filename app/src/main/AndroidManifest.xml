<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="android.support.VERSION"
            android:value="26.1.0"
            tools:replace="android:value" />

        <activity
            android:name=".Activities.DangNhapActivity"
            android:label="Login"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.MainActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Activities.BienBaoActivity"
            android:label="Biển báo"
            android:parentActivityName=".Activities.MainActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Activities.LyThuyetActivity"
            android:label="Lý thuyết"
            android:parentActivityName=".Activities.MainActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Activities.MeoGhiNhoActivity"
            android:label="Mẹo ghi nhớ"
            android:parentActivityName=".Activities.MainActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Activities.MeoThucHanhAActivity"
            android:label="Mẹo thực hành"
            android:launchMode="singleTop"
            android:parentActivityName=".Activities.MainActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Activities.MeoThucHanhBActivity"
            android:label="Mẹo thực hành"
            android:launchMode="singleTop"
            android:parentActivityName=".Activities.MainActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Activities.KinhNghiemThiAActivity"
            android:label="Kinh nghiệm thi"
            android:parentActivityName=".Activities.MeoThucHanhAActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Activities.KinhNghiemThiBActivity"
            android:label="Kinh nghiệm thi"
            android:parentActivityName=".Activities.MeoThucHanhBActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Activities.ThiSatHachActivity"
            android:label="Thi sát hạch"
            android:parentActivityName=".Activities.MainActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Activities.KetQuaActivity"
            android:label="Kết quả thi"
            android:launchMode="singleTop"
            android:parentActivityName=".Activities.MainActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Activities.XemLaiDapAnActivity"
            android:label="Xem lại đáp án"
            android:parentActivityName=".Activities.KetQuaActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Activities.LichSuBaiThiActivity"
            android:label="Lịch sử bài thi"
            android:parentActivityName=".Activities.MainActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>